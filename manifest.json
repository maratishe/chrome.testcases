{
	"manifest_version":2,
	"name":"Testcases",
	"description":"Test cases for various components",
	"version":"1.0",
	"permissions": ["tabs","storage","<all_urls>"],
	"browser_action":{
		"default_icon":"icon.png",
		"default_title":"Testcases",
		"default_popup":"testcases.float.html"
	},
	"background":{ "scripts":["serverless.js","testcases.background.js"]},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}